<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.ffxivtreasurechesthelper.FFXIVTreasureChestHelperController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText"/>
<!--    <Button text="Hello!" onAction="#onHelloButtonClick"/>-->
    <Label fx:id="title">Enter the treasure chest coordinates below.</Label>
    <TextField fx:id="inputCoordinate" onAction="#onCoordinateInput"/>
    <Label fx:id="zoneData"/>
</VBox>
